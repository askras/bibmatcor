%% Required fields: author, title, journal, year.
%% Optional fields: volume, number, pages, month, note.

@article{FirstAuthor_Magazine_Year,
  author     = "",
  title      = "",
  journal    = "",
  year       = "XXXX",
  %volume    = "",
  %number    = "",
  %pages     = "",
  %month     = "",
  %note      = "",
  %%crossref = "",
